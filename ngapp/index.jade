  body.container(ng-app="vinstonApp",ng-controller="appCtrl")
    // [if lt IE 7]>
      <p class="browsehappy">You are using an <strong>outdated</strong> browser. 
      Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
      ![endif]
    i.fa.fa-spinner.fa-spin.fa-2x(ng-hide="loaded")
    #inforow.row.inforow(ng-cloak,ng-show="loaded")
      nav.nav.nav-pills(role="navigation")
        img.activeInstitute.left.img-rounded(ng-click="institute.showModal()"
            ng-src="{{session.getActiveInstitute().image}}",alt="{{session.getActiveInstitute().name}}")
        button#semesterbutton.left.btn.btn-default {{semesterName}}
          p {{dauer}}
        button#loginbutton.btn.btn-default.right(ng-if="!auth.getUser()",ng-click="auth.showLoginModal()") Login
        button#userbutton.btn.btn-default.right(ng-if="auth.getUser()")
          span Hi, {{auth.getUser().name}}

        li.dropdown
          a.dropdown-toggle(href="#",data-toggle="dropdown") Student
            span.caret
          ul.dropdown-menu
            li
              a(href="#/student/main") Mein Stundenplan
            li
              a(href="#/student/bookRoom") Raum buchen
            li
              a(href="#/all/freeRoom") Freien Raum finden
            li
              a(href="#/all/vlvz") Vorlesungsverzeichnis
        li.dropdown(ng-if="auth.inGroup('docent')")
          a.dropdown-toggle(href="#",data-toggle="dropdown") Dozent
            b.caret
          ul.dropdown-menu
            li
              a(href="#/docent/main") Mein Stundenplan
            li
              a(href="#/student/bookRoom") Raum buchen
            li
              a(href="#/docent/registration") Zumeldung
        li.dropdown(ng-if="auth.inGroup('admin')")
          a.dropdown-toggle(href="#",data-toggle="dropdown") Administrator
            b.caret
          ul.dropdown-menu
            li 
              a(href="#/admin/rooms") RÃ¤ume
            li 
              a(href="#/admin/docents") Dozenten
            li 
              a(href="#/admin/structure") Struktur
        li.dropdown(ng-if="auth.inGroup('root')")
          a.dropdown-toggle(href="#",data-toggle="dropdown") Root
            b.caret
          ul.dropdown-menu
            li 
              a(href="#/root/helpUser") Benutzer helfen
            li 
              a(href="#/root/planning") Planung
            li 
              a(href="#/root/his") HIS export
            li 
              a(href="#/root/import") Datenbank import
            li 
              a(href="#/root/users") Benutzer verwalten
            li 
              a(href="#/root/appConfig") App konfiguration
            li 
              a(href="#/root/serverConfig") Server konfiguration
            
    .row(ng-view,ng-cloak,ng-show="loaded")
    toaster-container(toaster-options="{'time-out': 3000,'position-class':'toast-bottom-right'}")
