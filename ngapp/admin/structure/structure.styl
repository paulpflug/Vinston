.angular-ui-tree-handle {
  background: #f8faff;
  border: 1px solid #dae2ea;
  color: #7c9eb2;
  padding: 10px 10px;
  width: 650px
}
.angular-ui-tree-handle input {
  display: inline
  background-color: #f8faff;
  margin: 0 10px
  border: none;
  box-shadow: none;
  transition: none;
}
.angular-ui-tree-handle input.name{
  width: 360px
}
.angular-ui-tree-handle input.abbr{
  width: 100px
}
.angular-ui-tree-handle:hover {
  color: #438eb9;
  background: #f4f6f7;
  border-color: #dce2e8;
}

.angular-ui-tree-placeholder {
  background: #f0f9ff;
  border: 2px dashed #bed2db;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

